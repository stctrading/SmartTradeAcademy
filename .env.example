# STC Trading System - Configuraci√≥n de Entorno
# Copiar como .env y completar con valores reales

# IQ Option API
IQ_USERNAME=your_username
IQ_PASSWORD=your_password
IQ_MODE=PRACTICE
IQ_ACTIVE_ID=1

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
FLASK_PORT=5001

# MT5 Configuration (opcional)
MT5_LOGIN=your_mt5_login
MT5_PASSWORD=your_mt5_password
MT5_SERVER=your_mt5_server

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/stc_trading.log

# TradingView Charts
TRADINGVIEW_LOCAL=true
TRADINGVIEW_CDN_FALLBACK=true
